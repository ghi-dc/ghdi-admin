        "mpdf/mpdf": "^8.0",

        "sabre/xml": "^1.5",
        "servo/fluidxml": "@stable",

        "masterminds/html5": "2.*",

        "hedii/zotero-api": "@stable",

        "citation-style-language/locales":"@dev",
        "seboettg/citeproc-php": "^2.1",
        "fale/isbn": "3.*",

        "semsol/arc2": "2.3.*",
        "easyrdf/easyrdf": "0.9.*",
        "ml/json-ld": "1.0.*",

        "lstrojny/fxmlrpc": "@stable",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message": "^1.6",
        "guzzlehttp/psr7": "^1.4",
        "cocur/slugify": "^3.1",

        "textrazor/textrazor-php": "*@dev"
