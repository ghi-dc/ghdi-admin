{
    "config": {
        "platform": {
            "php": "7.1.3"
        }
    },

    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "citation-style-language/locales",
                "version":"1.0.0",
                "source": {
                    "type": "git",
                    "url": "https://github.com/citation-style-language/locales.git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "citation-style-language/styles-distribution",
                "version":"1.0.0",
                "source": {
                    "type": "git",
                    "url": "https://github.com/citation-style-language/styles-distribution.git",
                    "reference": "master"
                }
            }
        }
    ],

    "require": {
        "symfony/config": "^4.2",
        "symfony/finder": "^4.2",
        "symfony/filesystem": "^4.2",
        "symfony/yaml": "^4.2",
        "symfony/expression-language": "^4.2",
        "symfony/console": "^4.2",
        "symfony/process": "^4.2",
        "symfony/framework-bundle": "^4.2",

        "symfony/http-foundation": "^4.2",
        "symfony/event-dispatcher": "^4.2",
        "symfony/cache": "^4.2",
        "symfony/routing": "^4.2",
        "jms/i18n-routing-bundle": "^3.0",
        "jms/translation-bundle": "^1.4",

        "symfony/inflector": "^4.2",
        "symfony/property-access": "^4.2",
